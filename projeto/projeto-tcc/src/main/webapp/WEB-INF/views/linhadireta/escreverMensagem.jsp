<div id="conteudoPagina">
	<h2>Linha Direta</h2>
	<br>
	<table cellspacing="10">
		<tr>
			<td>Destinatário:</td>
			<td><select type="text" id="tipoMedicamento">
					<option tabindex="1"></option>
					<option>Unidade 1</option>
					<option>Unidade 2</option>
					<option>Unidade 3</option>
			</select></td>
		</tr>
		<tr>
			<td>Assunto:</td>
			<td><input type="text" id="assunto" /></td>
		</tr>
		<tr>
			<td>Mensagem:</td>
			<td><textarea name="textarea" id="textarea" cols="45" rows="5"
					required /></td>
		</tr>
		<tr>
			<td></td>
			<td><input type="button" value="Enviar"
				onclick="javascrit:submeter()" class="btnSubmit" /></td>
		</tr>

	</table>
	<tr>
</div>